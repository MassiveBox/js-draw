{
	"entryPoints": [
		"./packages/js-draw/src/lib.ts",
		"./packages/math/src/lib.ts",
		"./packages/material-icons/src/lib.ts"
	],
	"entryPointStrategy": "resolve",
	"name": "js-draw",
	"out": "docs/typedoc/",
	"plugin": [ "@js-draw/typedoc-extensions" ],

	"exclude": [
		"**/*.test.ts",
		"node_modules/**",
		"**/dist/**",
		"**/dist-test/**",
		"src/testing/"
	],

	"excludePrivate": true,
	"excludeInternal": true,
	"includeVersion": false,
	"commentStyle": "all",

	"readme": "README.md",
	"tsconfig": "tsconfig-typedoc.json",

	"validation": {
		"notExported": false,
		"invalidLink": true,
		"notDocumented": false
	}
}
