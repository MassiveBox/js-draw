{
	"entryPoints": [
		"./src/"
	],
	"exclude": [
		"**/*.test.ts"
	],
	"excludePrivate": true,
	"excludeInternal": true,
	"commentStyle": "all",
	"readme": "README.md",
	"entryPointStrategy": "expand",
	"out": "docs/typedoc/",

	"validation": {
		"notExported": false,
		"invalidLink": true,
		"notDocumented": false
	}
}