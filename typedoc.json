{
	"entryPoints": [
		"./packages/js-draw",
		"./packages/math",
	],
	"entryPointStrategy": "packages",
	"name": "js-draw",
	"out": "docs/typedoc/",
	"includeVersion": false,
	"plugin": [ "@js-draw/typedoc-extensions" ],

	"readme": "README.md",
