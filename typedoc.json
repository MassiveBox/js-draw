{
	"entryPoints": [
		"./src/"
	],
	"exclude": [
		"**/*.test.ts",
		"node_modules/**",
		"dist/",
		"dist-test/",
		"src/testing/"
	],
	"excludePrivate": true,
	"excludeInternal": true,
	"commentStyle": "all",
	"readme": "README.md",
	"entryPointStrategy": "expand",
	"out": "docs/typedoc/",

	"validation": {
		"notExported": false,
		"invalidLink": true,
		"notDocumented": false
	}
}