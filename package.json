{
  "name": "js-draw",
  "version": "0.0.1",
  "description": "Draw pictures using a pen, touchscreen, or mouse! JS-draw is a drawing library for JavaScript and TypeScript. ",
  "main": "index.ts",
  "repository": "https://github.com/personalizedrefrigerator/js-draw",
  "author": "Henry Heino <hheino@uw.edu>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "jest",
    "bundle": "ts-node scripts/bundle.ts",
    "watchBundle": "ts-node scripts/watchBundle.ts",
    "build": "yarn bundle",
    "watch": "yarn watchBundle",
    "lint": "eslint .",
    "linter-precommit": "eslint --fix --ext .js --ext .ts",
    "lint-staged": "lint-staged",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "dependencies": {
    "@melloware/coloris": "^0.16.0",
    "bezier-js": "^6.1.0"
  },
  "devDependencies": {
    "@types/bezier-js": "^4.1.0",
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.9",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "css-loader": "^6.7.1",
    "eslint": "^8.22.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "pinst": "^3.0.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.5",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0"
  }
}
